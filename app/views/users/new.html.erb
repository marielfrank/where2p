<h1>Sign Up</h1>
<%= form_for @user do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :email %><br>
    <%= f.email_field :email %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
    <%= f.submit 'Sign Up', class: 'btn cta' %>
<% end %>
<%= render 'sessions/oauth-link' %>